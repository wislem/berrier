{
    "name": "wislem/berrier",
    "description": "A fruitful crud generator",
    "keywords": [
        "Wislem",
        "Berrier",
        "Laravel",
        "Admin",
        "Panel"
    ],
    "homepage": "https://github.com/wislem/berrier",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Lazaridis",
            "email": "peter.lazaridis@gmail.com",
            "homepage": "http://29berries.gr",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "^5.2",
        "php" : ">=5.5.9",
        "propaganistas/laravel-translatable-bootforms": "~1.1",
        "devfactory/minify": "1.0.*",
        "caffeinated/menus": "~2.0",
        "kalnoy/nestedset": "^4.0",
        "illuminate/console": "^5.2",
        "illuminate/pagination": "^5.2",
        "folklore/image": "^0.3.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Wislem\\Berrier\\": "src"
        },
        "files": ["src\\helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "wislem\\berrier\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
